{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.ts"
  ],
  "coverageDirectory": "<rootDir>/coverage/",
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "fakeTimers": {
    "enableGlobally": true
  },
  "preset": "ts-jest",
  "setupFiles": [
    "./test/manualMocks.ts"
  ],
  "testEnvironment": "jsdom"
}
